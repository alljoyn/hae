<node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.allseenalliance.org/schemas/introspect.xsd">
    <interface name="org.alljoyn.Hae.UserInterfaceSettings">
        <annotation name="org.alljoyn.Bus.Secure" value="false"/>
        <property name="Version" type="q" access="read"/>
        <property name="DisplayTemperatureUnit" type="b" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="SupportedDisplayTemperatureUnitSettings" type="ab" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
            <property name="DisplayLanguage" type="s" access="read">
                <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
            </property>
            <property name="SupportedDisplayLanguageSettings" type="as" access="read">
                <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
                <property name="DisplayTimeFormat" type="b" access="read">
                    <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
                </property>
                <property name="SupportedDisplayTimeFormatSettings" type="ab" access="read">
                    <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
                </property>
                <method name="SetDisplayTemperatureUnit">
                    <arg name="displayTemperatureUnit" type="b" direction="in"/>
                </method>
                <method name="SetDisplayLaunguage">
                    <arg name="displayLanguange" type="s" direction="in"/>
                </method>
                <method name="SetDisplayTimeFormat">
                    <arg name="displayTimeFormat" type="b" direction="in"/>
                </method>
            </interface>
        </node>