<node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.allseenalliance.org/schemas/introspect.xsd">
    <interface name="org.alljoyn.SmartSpaces.Devices.RobotCleaner">
        <description language="en">This interface provides capabilities to control and monitor robot cleaner specific functions and resources.</description>
        <annotation name="org.alljoyn.Bus.Secure" value="true"/>
        <struct name="OperationalCycleDescription">
            <field name="cycle" type="q"/>
            <field name="name" type="s"/>
            <field name="description" type="s"/>
        </struct>
        <struct name="CyclePhaseDescription">
            <field name="phase" type="y"/>
            <field name="name" type="s"/>
            <field name="description" type="s"/>
        </struct>
        <property name="Version" type="q" access="read">
            <description language="en">Interface version</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="OperationalCycle" type="q" access="read">
            <description language="en">Current operational cycle</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="SupportedOperationalCycles" type="aq" access="read">
            <description language="en">Array of supported operational cycles</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
        </property>
        <property name="SelectableOperationalCycles" type="aq" access="read">
            <description language="en">Array of selectable operational cycle indetifiers</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="CyclePhase" type="y" access="read">
            <description language="en">Current cycle phase</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="SupportedCyclePhases" type="ay" access="read">
            <description language="en">Array of supported cycle phases</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
        </property>
        <property name="SelectableCyclePhases" type="ay" access="read">
            <description language="en">Array of selectable cycle phases</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="TurboMode" type="y" access="read">
            <description language="en">Represent enabled/disabled state of more powerful cleaning mode.</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="RepeatMode" type="y" access="read">
            <description language="en">Represent enabled/disabled state of the repeat mode.If enabled, robot cleaner will continously clean until the battery runs off.</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <method name="GetOperationalCyclesDescription">
            <description language="en">Invoke this method to get operational cycles descriptions</description>
            <arg name="language" type="s" direction="in"/>
            <arg name="cyclesDescription" type="a[OperationalCycleDescription]" direction="out"/>
        </method>
        <method name="GetCyclePhasesDescription">
            <description language="en">Invoke this method to get cyecl phases descriptions is string format</description>
            <arg name="language" type="s" direction="in"/>
            <arg name="phasesDescription" type="a[CyclePhaseDescription]" direction="out"/>
        </method>
    </interface>
</node>
