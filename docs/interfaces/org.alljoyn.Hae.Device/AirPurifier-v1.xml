<node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.allseenalliance.org/schemas/introspect.xsd">
    <interface name="org.alljoyn.Hae.Device.AirPurifier">
        <annotation name="org.alljoyn.Bus.Secure" value="false"/>
        <property name="Version" type="q" access="read"/>            
        <property name="OpticalCatalystMode" type="y" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="IonEmissionMode" type="y" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="SterilizeMode" type="y" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="PowerfulMode" type="y" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="SleepMode" type="y" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="AutoMode" type="y" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="WaterAmountLevel" type="y" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="MaxWaterAmountLevel" type="y" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
        </property>
        <method name="ChangeOpticalCatalystMode">
             <arg name="opticalCatalystMode" type="y" direction="in"/>
        </method>
        <method name="ChangeIonEmissionMode">
             <arg name="ionEmissionMode" type="y" direction="in"/>
        </method>
        <method name="ChangeSterilizeMode">
             <arg name="sterilizeMode" type="y" direction="in"/>
        </method>
        <method name="ChangePowerfulMode">
             <arg name="powerfulMode" type="y" direction="in"/>
        </method>
        <method name="ChangeSleepMode">
             <arg name="sleepMode" type="y" direction="in"/>
        </method>
        <method name="ChangeAutoMode">
             <arg name="autoMode" type="y" direction="in"/>
        </method>
    </interface>
</node>
