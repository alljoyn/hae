<node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.allseenalliance.org/schemas/introspect.xsd">
    <interface name="org.alljoyn.Hae.Device.RobotCleaner">
        <annotation name="org.alljoyn.Bus.Secure" value="false"/>
        <struct name="OperationalCycleDescriptor">
            <field name="cycleId" type="q"/>
            <field name="name" type="s"/>
            <field name="description" type="s"/>
        </struct>
        <struct name="CyclePhaseDescriptor">
            <field name="phaseId" type="y"/>
            <field name="name" type="s"/>
            <field name="description" type="s"/>
        </struct>        
        <property name="Version" type="q" access="read"/>
        <property name="OperationalCycleId" type="q" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="SupportedOperationalCycleIds" type="aq" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
        </property>
        <property name="SelectableOperationalCycleIds" type="aq" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="CyclePhaseId" type="y" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="SupportedCyclePhaseIds" type="ay" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
        </property>
        <property name="SelectableCyclePhaseIds" type="ay" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="TurboMode" type="y" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="RepeatMode" type="y" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <method name="SetOperationalCycleId">
             <arg name="operationalCycleId" type="q" direction="in"/>
        </method>
        <method name="GetOperationalCyclesDescription">
             <arg name="language" type="s" direction="in"/>
             <arg name="cyclesDescription" type="a[OperationalCycleDescriptor]" direction="out"/>
        </method>
        <method name="GetCyclePhasesDescription">
             <arg name="language" type="s" direction="in"/>
             <arg name="phasesDescription" type="a[CyclePhaseDescriptor]" direction="out"/>
        </method>        
        <method name="EnableTurboMode">
             <arg name="turboMode" type="b" direction="in"/>
        </method>
        <method name="EnableRepeatMode">
             <arg name="repeatMode" type="b" direction="in"/>
        </method>
    </interface>
</node>
