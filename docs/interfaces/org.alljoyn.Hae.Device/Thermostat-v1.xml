<node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.allseenalliance.org/schemas/introspect.xsd">
    <interface name="org.alljoyn.Hae.Device.Thermostat">
        <annotation name="org.alljoyn.Bus.Secure" value="false"/>
        <struct name="OperationalModeDescriptor">
            <field name="modeId" type="q"/>
            <field name="name" type="s"/>
            <field name="description" type="s"/>
        </struct>
        <property name="Version" type="q" access="read"/>
        <property name="OperationalModeId" type="q" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="SupportedOperationalModeIds" type="aq" access="read">
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <method name="SetOperationModeId">
             <arg name="operationModeId" type="q" direction="in"/>
        </method>
        <method name="GetOperationalModesDescription">
            <arg name="languageTag" type="s" direction="in"/>
            <arg name="modesDescription" type="a[OperationalModeDescriptor]" direction="out"/>
        </method>
    </interface>
</node>
